{
  "name": "Tibia Fandom Wiki Speedup",
  "description": "Blocks all network communication to external domains on Tibia Fandom pages.",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback"],
  "host_permissions": ["https://tibia.fandom.com/*"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules_1.json"
      }
    ]
  }
}
